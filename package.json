{
  "name": "nulladdict.com",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write --ignore-unknown --plugin-search-dir=. .",
    "format:this": "prettier --write --ignore-unknown --plugin-search-dir=."
  },
  "devDependencies": {
    "@csstools/postcss-oklab-function": "^2.2.1",
    "@sveltejs/adapter-cloudflare": "^2.2.0",
    "@sveltejs/kit": "^1.15.7",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "simple-git-hooks": "^2.8.1",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-preprocess": "^5.0.3",
    "typescript": "^5.0.4",
    "workbox-core": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "wrangler": "^2.16.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "npm run format:this --",
    "src/**/*.{d.ts,js,ts,svelte}": "npm run check"
  }
}
