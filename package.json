{
  "name": "nulladdict.com",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "npm run build && npx wrangler pages dev .svelte-kit/cloudflare",
    "upgrade": "npx npm-check@latest -u && npm update",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "fmt": "prettier --write --ignore-unknown .",
    "fmt:this": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^2.2.2",
    "@sveltejs/kit": "^1.16.3",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.14",
    "lint-staged": "^13.2.2",
    "mdsvex": "^0.10.6",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "simple-git-hooks": "^2.8.1",
    "svelte": "^3.59.1",
    "svelte-check": "^3.3.2",
    "svelte-preprocess": "^5.0.3",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "workbox-core": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "wrangler": "^3.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "npm run fmt:this --",
    "src/**/*.{d.ts,js,ts,svelte}": "npm run check"
  }
}
