{
  "name": "nulladdict.com",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write --ignore-unknown --plugin-search-dir=. .",
    "format:this": "prettier --write --ignore-unknown --plugin-search-dir=."
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "next",
    "@sveltejs/kit": "next",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "simple-git-hooks": "^2.8.1",
    "svelte": "^3.53.1",
    "svelte-check": "^2.10.0",
    "svelte-preprocess": "^4.10.7",
    "typescript": "^4.9.3",
    "workbox-core": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "wrangler": "^2.5.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "npm run format:this --",
    "src/**/*.{d.ts,js,ts,svelte}": "npm run check"
  }
}
